{
  "name": "ppsus-ears-node",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node src/scripts/StartServer.js",
    "start:dev": "cd build && npm start",
    "dev": "npm run build:env && npm run start:dev",
    "dev:nodemon": "nodemon -L --ignore node_modules --ignore build --ext ts --exec npm run dev",
    "doc": "node build/src/scripts/SwaggerGenerate.js",
    "env": "node build/src/scripts/CopyEnv.js",
    "build": "npm run clean build && tsc --project ./",
    "build:prod": "npm run build && npm run doc",
    "build:env": "npm run build && npm run doc && npm run env",
    "clean": "ts-node src/scripts/Clean.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "lint": "eslint \"./src/**\" --ext .ts",
    "lint:fix": "eslint \"./src/**\" --ext .ts --fix"
  },
  "engines": {
    "node": "^16"
  },
  "dependencies": {
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "excel4node": "^1.8.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger": "^0.7.5",
    "swagger-autogen": "^2.21.5",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.2.45",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.15",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.22.0",
    "eslint-plugin-sort": "^2.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
