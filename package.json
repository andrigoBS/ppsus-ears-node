{
  "name": "ppsus-ears-node",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node src/scripts/StartServer.js",
    "start:dev": "cd build && npm start",
    "dev": "npm run build:env && npm run start:dev",
    "dev:nodemon": "nodemon -L --ignore node_modules --ignore build --ext ts --exec npm run dev",
    "env": "node build/src/scripts/CopyEnv.js",
    "build": "npm run clean build && tsc --project ./",
    "build:prod": "npm run build",
    "build:env": "npm run build:prod && npm run env",
    "clean": "ts-node src/scripts/Clean.ts",
    "typeorm:dev": "node --require ts-node/register ../node_modules/typeorm/cli.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "lint": "eslint \"./src/**\" --ext .ts",
    "lint:fix": "eslint \"./src/**\" --ext .ts --fix"
  },
  "engines": {
    "node": "^16"
  },
  "dependencies": {
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "excel4node": "^1.8.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "2.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "0.2.45",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.14",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-plugin-sort": "^2.7.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
