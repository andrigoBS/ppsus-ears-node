{
  "name": "ppsus-ears-node",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build && node --inspect=0.0.0.0 build/src/server.js",
    "build": "tsc --project ./ && node build/src/swaggerGenerate.js && copyfiles public/* build && copyfiles package*.json build",
    "dev": "nodemon -L --ignore public --ignore node_modules --ignore build --ext ts --exec npm run start",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mysql2": "^2.3.0",
    "nodemon": "^2.0.12",
    "reflect-metadata": "^0.1.13",
    "swagger": "^0.7.5",
    "swagger-autogen": "^2.11.2",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/node": "^14.17.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.1",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
