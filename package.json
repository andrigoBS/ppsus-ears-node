{
  "name": "ppsus-ears-node",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build && node build/src/server.js",
    "build": "tsc --project ./ && node build/src/swaggerGenerate.js && copyfiles public/* build && copyfiles package.json build",
    "dev": "nodemon --ignore public --ignore node_modules --ext ts npm run start",
    "prod": "nodemon --ignore node_modules --ext js src/server.js"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.12",
    "sequelize": "^6.6.5",
    "swagger": "^0.7.5",
    "swagger-autogen": "^2.11.2",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.17.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/body-parser": "^1.19.0",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
